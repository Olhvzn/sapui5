<mvc:View
	controllerName="sap.m.sample.TreeExpandMulti.Page"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Toolbar>
	<content>
		<ToolbarSpacer/>
		<Button id="expandMulti" text="expand selected nodes" press="onExpandMultiPress"/>
		<Button id="collapseMulti" text="collapse selected nodes" press="onCollapseMultiPress"/>
	</content>
	</Toolbar>

	<Tree
		id="Tree"
		items="{path: '/'}"
		mode="MultiSelect">
		<StandardTreeItem title="{text}"/>
	</Tree>
</mvc:View>