<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay480_Trial"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Address"
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="3"
			columnsL="2"
			columnsM="2"
			singleContainerFullSize="false" >
			<f:content>
				<core:Title text="Office" />
				<Label text="Name" />
				<Text text="{SupplierName}" />
				<Label text="Street/No." />
				<Text text="{Street} {HouseNumber}" />
				<Label text="ZIP Code/City" />
				<Text text="{ZIPCode} {City}" />
				<Label text="Country" />
				<Text text="{Country}" />
				<core:Title text="Online" />
				<Label text="Web" />
				<Text text="{Url}" />
				<Label text="Twitter" />
				<Text text="{Twitter}" />
				<Label text="Email" />
				<Text text="{Email}" />
				<Label text="Tel." />
				<Text text="{Tel}" />
				<Label text="SMS" />
				<Text text="{Sms}" />
				<core:Title text="Other" />
				<Label text="Rating" />
				<RatingIndicator value="{Rating}" displayOnly="true" />
				<Label text="Disposable" />
				<ProgressIndicator percentValue="{Disposable}" displayOnly="true" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>